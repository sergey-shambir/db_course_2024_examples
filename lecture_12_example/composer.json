{
  "config": {
    "platform": {
      "php": "8.2.2"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "tests": "php vendor/bin/phpunit tests --bootstrap=tests/bootstrap.php"
  },
  "require": {
    "ext-pdo": "*",
    "ext-ctype": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "11.1.1"
  }
}
