{
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\App\\": "tests/"
    }
  },
  "scripts": {
    "tests": "php vendor/bin/phpunit tests --bootstrap=tests/bootstrap.php",
    "component-tests": "php vendor/bin/phpunit tests/Component --bootstrap=tests/bootstrap.php"
  },
  "require": {
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.23"
  }
}
